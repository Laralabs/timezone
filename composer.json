{
  "name": "laralabs/timezone",
  "description": "Laravel package for easy bi-directional conversion of DateTime into a variety of formats and locales",
  "keywords": [
    "laralabs",
    "laravel",
    "timezone",
    "conversion",
    "locale",
    "presenter"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Matt Clinton",
      "email": "matt@laralabs.uk",
      "homepage": "https://github.com/clnt"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^5.5|^6.0",
    "jenssegers/date": "^3.4.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.4|^4.0",
    "phpunit/phpunit": ">6.0"
  },
  "autoload": {
    "psr-4": {
      "Laralabs\\Timezone\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Laralabs\\Timezone\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laralabs\\Timezone\\TimezoneServiceProvider"
      ],
      "aliases": {
        "Timezone": "Laralabs\\Timezone\\Facades\\Timezone"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}